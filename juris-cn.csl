<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.1mlz1" class="note">
  <info>
    <title>People's Republic of China</title>
    <id>http://citationstylist.org/modules/juris-zh</id>
    <link href="http://citationstylist/modules/juris-at" rel="self"/>
    <link href="http://juris-m.github.io" rel="documentation"/>
    <author>
      <name>Frank Bennett</name>
      <email>bennett@nagoya-u.jp</email>
    </author>
    <category citation-format="note"/>
    <category field="law"/>
    <summary>Juris-M style module for Austria</summary>
    <updated>2013-01-26T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
    <law-module types="legal_case regulation legislation"/>
  </info>
  
  <macro name="juris-pretitle">
    <!-- Fragment to precede title (rare) -->
    <text value=""/>
  </macro>

  <macro name="juris-title">
    <!-- The title form for full citations (of all types) -->
    <choose>
      <if type="legal_case">
        <text variable="title"/>
      </if>
    </choose>
  </macro>

  <macro name="juris-main">
    <choose>
      <if type="regulation legislation" match="any">
        <group delimiter=" ">
          <text variable="title"/>
          <group delimiter=", " prefix="(" suffix=")">
            <group delimiter=" ">
              <text value="promulgated by the"/>
              <names variable="authority">
                <name/>
                <institution/>
              </names>
            </group>
            <date variable="issued" form="text" date-parts="year-month-day"/>
            <group delimiter=" ">
              <text value="effective"/>
              <date variable="available-date" form="text" date-parts="year-month-day"/>
            </group>
          </group>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="juris-tail">
    <!-- Full form of the portion of the citation that follows a pinpoint locator, if any (for all types) -->
    <choose>
      <if type="legal_case">
        <group delimiter=" " prefix="(" suffix=")">
          <names variable="authority">
            <name/>
            <institution institution-parts="short"/>
          </names>
          <date variable="issued" form="text" date-parts="year-month-day"/>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="juris-pretitle-short">
    <!-- Fragment to precede short title (rare) -->
    <text value=""/>
  </macro>

  <macro name="juris-title-short">
    <!-- The title form for subsequent citations (of all types) -->
    <text value=""/>
  </macro>

  <macro name="juris-main-short">
    <text value=""/>
  </macro>

  <macro name="juris-tail-short">
    <!-- Short form of the portion of the citation that follows a pinpoint locator, if any (for all types) -->
    <text value=""/>
  </macro>

  <macro name="juris-locator">
    <group delimiter=" ">
      <choose>
        <if locator="page">
          <text value="at"/>
        </if>
        <else>
          <label variable="locator"/>
        </else>
      </choose>
      <number variable="locator"/>
    </group>
  </macro>
  
  <citation>
    <layout>
    </layout>
  </citation>

</style>
