<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.1mlz1" class="note">
  <info>
    <title>United Nations</title>
    <id>http://citationstylist.org/modules/juris-un.int</id>
    <link href="http://citationstylist/modules/juris-un.int" rel="self"/>
    <link href="http://juris-m.github.io" rel="documentation"/>
    <author>
      <name>Frank Bennett</name>
      <email>bennett@nagoya-u.jp</email>
    </author>
    <category citation-format="note"/>
    <category field="law"/>
    <summary>Juris-M style module for United Nations</summary>
    <updated>2013-01-26T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
    <law-module types="bill"/>
  </info>

  <macro name="juris-pretitle">
    <text value=""/>
  </macro>

  <macro name="juris-title">
    <text value=""/>
  </macro>

  <macro name="juris-main">
    <choose>
      <if type="bill">
        <group delimiter=" ">
          <names variable="authority">
            <name/>
            <institution institution-parts="short"/>
          </names>
          <text variable="event" form="short"/>
          <number variable="number"/>
          <date variable="issued" form="text" date-parts="year-month-day" prefix="(" suffix=")"/>
          <choose>
            <if variable="container-title" match="all">
              <text variable="container-title" form="short"/>
              <group delimiter=", ">
                <group delimiter=" ">
                  <group delimiter=" ">
                    <number variable="chapter-number" form="ordinal"/>
                    <text value="Session"/>
                  </group>
                  <group delimiter=" ">
                    <text value="Supp."/>
                    <number variable="supplement"/>
                  </group>
                </group>
                <number variable="page"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <text value="U.N. Doc."/>
                <text variable="archive_location"/>
              </group>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="juris-tail">
    <text value=""/>
  </macro>
  
  <macro name="juris-pretitle-short">
    <text value=""/>
  </macro>

  <macro name="juris-title-short">
    <text value=""/>
  </macro>

  <macro name="juris-main-short">
    <choose>
      <if type="bill">
        <group delimiter=" ">
          <names variable="authority">
            <name/>
            <institution institution-parts="short"/>
          </names>
          <text variable="event" form="short"/>
          <number variable="number"/>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="juris-tail-short">
    <text value=""/>
  </macro>
  <macro name="juris-locator">
    <group delimiter=" ">
      <choose>
        <if locator="page"/>
        <else>
          <label variable="locator"/>
        </else>
      </choose>
      <number variable="locator"/>
    </group>
  </macro>
  <citation>
    <layout>
    </layout>
  </citation>

</style>
